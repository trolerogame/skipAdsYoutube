{
    "manifest_version": 3,
    "name": "skipAd",
    "description": "YouTube Skip Ads Extension",
    "version": "1.0",
    "action": {
      "default_popup": "hello.html",
      "default_icon": "icon.png"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "content_scripts": [
    {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"]
    }
    ],
    "icons": {
        "48": "icon.png"
    }
  }